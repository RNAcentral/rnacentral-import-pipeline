params {
  r2dt {
    run = true
    sequence_chunks = 4000
    data_chunk_size = 1024 * 1000 * 1000
    tablename = 'traveler_sequences_to_analyze'
    publish = "$baseDir/r2dt"
    container = 'rnacentral/r2dt:latest'

    find_possible {
      memory = '4GB'
      max_len = 10000
    }

    layout {
      memory = '8GB'
    }

    store {
      memory = '4GB'
    }
  }
}
